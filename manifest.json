{
    "manifest_version": 2,
    "name": "Youtube.getMp3()",
    "description": "This extension download a mp3 file from a youtube link.",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "Download Youtube Mp3"
    },
    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/",
        "tabs",
        "http://*/*",
        "https://*/*",
        "downloads",
        "<all_urls>"
    ]
}